colors=['red','maroon','cyan']
markers=['o','o','o']
classlabel=['Iris-setosa','Iris-versicolor','Iris-virginica']

kmeans=KMeans(n_clusters=3).fit(x)
plt.scatter(kmeans.cluster_centers_[:,0],kmeans.cluster_centers_[:,1],s=250,c='black',label='Centroids')
for i,l in enumerate(kmeans.labels_):
    plt.plot(arr1[i],arr2[i],color=colors[l],marker=markers[l])
plt.xlabel('Width')
plt.ylabel('Lenght')
plt.legend()
plt.show()
